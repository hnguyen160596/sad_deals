{
  "name": "react-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun x vite --host 0.0.0.0",
    "build": "bun x vite build && bun run copy-netlify-functions",
    "build:production": "bun x vite build && bun run copy-netlify-functions",
    "netlify-build": "bun x vite build && bun run copy-netlify-functions",
    "copy-netlify-functions": "node copy-netlify-functions.js",
    "lint": "bunx biome lint --write && bunx tsc --noEmit",
    "format": "bunx biome format --write",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "@types/chart.js": "^4.0.1",
    "aws4": "^1.13.2",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "cheerio": "^1.0.0",
    "emoji-picker-react": "^4.12.2",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "image-size": "^2.0.2",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^6.9.10",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.3",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.34.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.4",
    "workbox-window": "^7.3.0"
  }
}
